{
    "name": "skymailbr/plesk-xml-api",
    "description": "PHP object-oriented library for Plesk API-RPC. forked >> plesk/api-php-lib",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lucas Stevanelli Marin",
            "email": "lucasmarin@gmail.com"
        }

    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "squizlabs/php_codesniffer": "~3.0",
        "ext-simplexml": "*",
        "ext-curl": "*",
        "vlucas/phpdotenv": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "PleskX\\": "src/PleskX/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    }
}
